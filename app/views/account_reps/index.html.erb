<%- model_class = AccountRep -%>
<div class="page-header">
  <h1>Account Reps for <%= link_to @user.title.to_s, @user unless @user.title.nil? %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th style="width: 35%"><%= model_class.human_attribute_name(:user_id) %></th>
      <th style="width: 25%"><%= model_class.human_attribute_name(:account_id) %></th>
      <th style="width: 20%"><%= model_class.human_attribute_name(:rep_level) %></th>
      <th style="width: 20%"><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody class="rep_list">
    <%= render 'list' %>
  </tbody>
</table>

<%= link_to 'Back', users_path, :class => 'btn' %>
